<!DOCTYPE html>
<html>
  <head>
    <title>Carshop</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  	<header>
	  	<thead>
		<tr>
		  <th><%= link_to 'Log in', login_index_path %></th>
		  <th><%= link_to 'Users', users_path %></th>
		  <th><%= link_to 'Car models', carmodels_path %></th>
		  <th><%= link_to 'Employees', employees_path %></th>
		  <th><%= link_to 'Sales', sales_path %></th>
		  <th colspan="3"></th>
		</tr>
	  </thead>
    </header>
    
    <%= yield %>
  </body>
</html>
